<li>{{rental.owner}}'s {{rental.type}} in {{rental.city}}
  {{#if isImageShowing}}
    <p><img src={{rental.image}} alt={{rental.type}} {{action 'imageHide'}}></p>
  {{else}}
    <button {{action 'imageShow'}} class="btn btn-primary">Show Image</button>
  {{/if}}
  <button {{action 'delete' rental}} class="btn btn-danger">Delete</button>
</li>
